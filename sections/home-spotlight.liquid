{%- capture flickity_options -%}
{
  "prevNextButtons": true,
  "wrapAround": true,
  "dragThreshold": 15,
  "pauseAutoPlayOnHover": false,
  "autoPlay": {% if section.settings.autoplay %}{{ section.settings.cycle_speed | times: 1000 }}{% else %}false{% endif %},
  "pageDots": true
}
{%- endcapture -%}

{% if localization.country.iso_code == section.settings.location or section.settings.location == 'both' %}
<section class="Section Home__Spotlight__main" id="section-{{ section.id }}"   {% if section.blocks.size > 1 %}  data-flickity='{{ flickity_options }}' {% endif %}>
  {% for block in section.blocks %}
    <div class=" Home__Spotlight__Container">
    <div class="Home__Spotlight__Wrap" {% if block.settings.color != blank %}style="background:{{ block.settings.color }}" {% endif %}>
    <div class="Bowl {% if block.settings.image_left %} Image__Left{% endif %}">
      {% assign image_url1 = block.settings.image1 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
          <img class="Image--lazyLoad hide-no-js"
                data-src="{{ image_url1 }}"
                data-optimumx="1.2"
                data-widths="[200, 300, 400]"
                data-sizes="auto"
                alt="{{ block.settings.image1.alt | escape }}">
      <noscript>
        <img src="{{ block.settings.image1 | img_url: '300x' }}" alt="{{ block.settings.image1.alt | escape }}">
      </noscript>
    </div>
    {% if block.settings.image2 != blank %}
    <div class="Hen">
      {% assign image_url2 = block.settings.image2 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
          <img class="Image--lazyLoad hide-no-js"
                data-src="{{ image_url2 }}"
                data-optimumx="1.2"
                data-widths="[200, 300, 400, 600, 800]"
                data-sizes="auto"
                alt="{{ block.settings.image2.alt | escape }}">
      <noscript>
        <img src="{{ block.settings.image2 | img_url: '300x' }}" alt="{{ block.settings.image2.alt | escape }}">
      </noscript>
    </div>
      {% endif %}
 {% if block.settings.image3 or block.settings.image3mobile != blank %}
    <div class="Pack">
  
      {% assign image_url3 = block.settings.image3 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
          <img class="Image--lazyLoad hide-no-js  {% if block.settings.image3mobile != blank %} hidden-phone {% endif %}"
                data-src="{{ image_url3 }}"
                data-optimumx="1.2"
                data-widths="[200, 300, 400, 600, 800]"
                data-sizes="auto"
                alt="{{ block.settings.image3.alt | escape }}">
      <noscript>
        <img src="{{ block.settings.image3 | img_url: '300x' }}" alt="{{ block.settings.image3.alt | escape }}">
      </noscript>
     {% if block.settings.image3mobile != blank %} 
    {% assign image_url3 = block.settings.image3mobile | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
          <img class="Image--lazyLoad hide-no-js hidden-tablet-and-up"
                data-src="{{ image_url3 }}"
                data-optimumx="1.2"
                data-widths="[200, 300, 400, 600, 800]"
                data-sizes="auto"
                alt="{{ block.settings.image3mobile.alt | escape }}">
      <noscript>
        <img src="{{ block.settings.image3mobile | img_url: '300x' }}" alt="{{ block.settings.image3mobile.alt | escape }}">
      </noscript>
       {% endif %}
    </div>
   {% endif %}
    <div class="Content__Wrap">
      <div class="Seal">
        {% assign image_url4 = block.settings.image4 | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
          <img class="Image--lazyLoad hide-no-js seal-animation"
                data-src="{{ image_url4 }}"
                data-optimumx="1.2"
                data-widths="[200, 300, 400]"
                data-sizes="auto"
                alt="{{ block.settings.image4.alt | escape }}">
      <noscript>
        <img src="{{ block.settings.image4 | img_url: '300x' }}" alt="{{ block.settings.image4.alt | escape }}" class="seal-animation">
      </noscript>
      </div>
      {%- if block.settings.subheading != blank or block.settings.title != blank -%}
        <header class="SectionHeader SectionHeader--center">
          {%- if block.settings.subheading != blank -%}
            <h3 class="SectionHeader__SubHeading Heading u-h6">{{ block.settings.subheading | escape }}</h3>
          {%- endif -%}
  
          {%- if block.settings.title != blank -%}
            <h2 class="SectionHeader__Heading Heading u-h1">{{ block.settings.title | escape }}</h2>
          {%- endif -%}
          {%- if block.settings.content != blank -%}
            {{ block.settings.content }}
          {%- endif -%}
          {%- if block.settings.link_text != blank -%}
            <a href="{{ block.settings.link_url }}" class="Button Button--primary">{{ block.settings.link_text | escape }}</a>
          {%- endif -%}
        </header>
      {%- endif -%}
    </div>

