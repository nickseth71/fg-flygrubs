{% schema %}
{
  "name": "Home Spotlight",
  "class": "shopify-section--bordered",
  "settings": [
	{
      "type": "select",
      "id": "location",
      "label": "Location",
      "options": [
        {
          "value": "US",
          "label": "US"
        },
        {
          "value": "DE",
          "label": "DE"
        },
      {
          "value": "both",
          "label": "Both"
        }
      ],
      "default": "both"
    },
      {
      "type": "checkbox",
      "id": "autoplay",
      "label": "Auto rotate between slides",
      "default": true
    },
    {
      "type": "range",
      "id": "cycle_speed",
      "min": 3,
      "max": 8,
      "step": 1,
      "unit": "sec",
      "label": "Change slides every",
      "default": 5
    }
   
  ],
  "blocks": [
    {
      "type": "image",
      "name": "image",
      "settings": [
       {
      "type": "color",
      "id": "color",
      "label": "BG color"
      },
        {
      "type": "text",
      "id": "subheading",
      "label": "Sub-heading",
      "default": "Sub-heading"
    },
    {
      "type": "text",
      "id": "title",
      "label": "Heading",
      "default": "Your HTML"
    },
    {
      "type": "richtext",
      "id": "content",
      "label": "Content",
      "default": "<p>Write your own custom content.</p>"
    },
    {
        "type": "image_picker",
        "id": "image1",
        "label": "Bowl Image",
        "info": "500 x 500px .png recommended | Bowl Image"
    },
    {
      "type": "checkbox",
      "id": "image_left",
      "label": "Image on left",
      "default": false
    },
    {
        "type": "image_picker",
        "id": "image2",
        "label": "Hen Image",
        "info": "500 x 500px .png recommended | Hen Image"
    },
    {
        "type": "image_picker",
        "id": "image3",
        "label": "Product Image",
        "info": "500 x 500px .png recommended | Product Image"
    },
         {
        "type": "image_picker",
        "id": "image3mobile",
        "label": "Product Image Mobile",
        "info": "500 x 500px .png recommended | Product Image"
    },
        
        {
        "type": "image_picker",
        "id": "image4",
        "label": "Seal Image",
        "info": "500 x 500px .png recommended | Seal Image"
    },{
        "type": "image_picker",
        "id": "image5",
        "label": "Flygrub grain Image",
        "info": "500 x 500px .png recommended | Flygrub grain Image"
    },
    {
        "type": "text",
        "id": "link_text",
        "label": "Button text"
    },
    {
        "type": "url",
        "id": "link_url",
        "label": "Button URL"
    }
      ]
        }
      ],
     "presets": [
    {
      "category": "image",
      "name": "Home Spotlight",
      "settings": {}
    }
  ]
}
{% endschema %}